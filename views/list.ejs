<%-include('header.ejs')%>

<div class="container">
    <div class="row">
        <% for(let i = 0; i < posts.length; i++){ %>
            <div class="col-md-4 mt-3">
                <div class="card p-3">
                    <h4><a href="post/<%=posts[i]._id%>"> <%= posts[i].title%> </a></h4>
                    <p><%= posts[i].content%></p>
                </div>
            </div>
        <% } %>
    </div>
</div>

<%-include('footer.ejs')%>